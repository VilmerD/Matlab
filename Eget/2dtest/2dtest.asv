%% Initializing
N = 10;
xx = linspace(0,1,N+2); xinner = xx(2:end-1);
yy = xx';                yinner = xinner';

%% Matrix
s = (mod(1:(N^2-1),N)~=0);
super = double(s);

M = -2*diag(ones(1,N^2)) + diag(super,1) + diag(super,-1) + diag(ones(1,N^2-4),4) + diag(ones(1,N^2-4), -4);

%% Initial function 
f = @(x,y) sin(3*pi*y)*sin(pi*x);
fm = f(xinner,yinner);              % Matrix
finner = fm(:);                          % Vector

%% 
y = M\finner;
ym = reshape(y, [N,N]);
[X,Y] = meshgrid(xx,yy);
